<!DOCTYPE html>
<html>
<body>
<h1>HTML5 Canvas</h1>

<canvas id="myCanvas" width="300" height="300" style="border:1px solid grey"></canvas>
<script>
  var c = document.getElementById("myCanvas");
  var ctx = c.getContext("2d");
  ctx.moveTo(100,0);
  ctx.lineTo(100,300);
  
  ctx.moveTo(200,0);
  ctx.lineTo(200,300);
  
  ctx.moveTo(0,100);
  ctx.lineTo(300,100);
  
   ctx.moveTo(0,200);
  ctx.lineTo(300,200);
  
  
  
  
  
  ctx.stroke();
</script>

</body>
</html>
